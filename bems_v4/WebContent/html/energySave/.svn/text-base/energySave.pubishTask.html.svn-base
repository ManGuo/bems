                          <option  selected="selected">==请选择==</option>

                          <option>暂不</option>

                          <option>一般</option>

                          <option>需要</option>

                          <option>急</option>

                          <option>很急</option>

                        </select></td>

					  </tr>

					  <tr>

					    <td nowrap align="right" height="120px">任务说明:</td>

					    <td colspan="3"><textarea id="textarea" name="textarea" rows="5" cols="80"></textarea></td>

					    </tr>

					  </table>

											
											
											
