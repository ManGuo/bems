@XmlRootElement(name="phone")
@XmlAccessorType(XmlAccessType.FIELD)
public class Phone {
	
	private int phoneId;
	private String phoneNumber;
	private int floorId;
	private String houseNumber;
	private String phonePower;
	private String personsresPoneSibleFor;//电话责任人
	public Phone(){
		
	}
	public Phone(
			int phoneId,
			String phoneNumber,
			int floorId,
			String houseNumber,
			String phonePower,
			String personsresPoneSibleFor
			){
		this.phoneId = phoneId;
		this.phoneNumber = phoneNumber;
		this.floorId=floorId;
		this.houseNumber = houseNumber;
		this.phonePower = phonePower;
		this.personsresPoneSibleFor = personsresPoneSibleFor;
	}
	public int getPhoneId() {
		return phoneId;
	}
	public void setPhoneId(int phoneId) {
		this.phoneId = phoneId;
	}
	public String getPhoneNumber() {
		return phoneNumber;
	}
	public void setPhoneNumber(String phoneNumber) {
		this.phoneNumber = phoneNumber;
	}
	public int getFloorId() {
		return floorId;
	}
	public void setFloorId(int floorId) {
		this.floorId = floorId;
	}
	public String getHouseNumber() {
		return houseNumber;
	}
	public void setHouseNumber(String houseNumber) {
		this.houseNumber = houseNumber;
	}
	public String getPhonePower() {
		return phonePower;
	}
	public void setPhonePower(String phonePower) {
		this.phonePower = phonePower;
	}
	public String getPersonsresPoneSibleFor() {
		return personsresPoneSibleFor;
	}
	public void setPersonsresPoneSibleFor(String personsresPoneSibleFor) {
		this.personsresPoneSibleFor = personsresPoneSibleFor;
	}
	
	

}
